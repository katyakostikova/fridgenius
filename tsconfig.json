{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "assets": [
        "src/assets/index.ts"
      ],
      "common/*": [
        "src/common/*"
      ],
      "components": [
        "src/components/index.ts"
      ],
      "hooks": [
        "src/hooks/index.ts"
      ],
      "navigation": [
        "src/navigation/index.ts"
      ],
      "screens": [
        "src/screens/index.ts"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}