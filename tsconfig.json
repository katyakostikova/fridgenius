{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "assets": [
        "./src/assets/*"
      ],
      "common": [
        "./src/common/*"
      ],
      "components": [
        "./src/components/*"
      ],
      "helpers": [
        "./src/helpers/*"
      ],
      "hooks": [
        "./src/hooks/*"
      ],
      "navigation": [
        "./src/navigation/*"
      ],
      "screens": [
        "./src/screens/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}